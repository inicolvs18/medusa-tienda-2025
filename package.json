{
  "name": "lsmarketabduceme",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "build:server": "copy medusa-config-server.js medusa-config.js && medusa build",
    "build:worker": "copy medusa-config-worker.js medusa-config.js && medusa build",
    "build:render": "copy medusa-config-render.js medusa-config.js && medusa build && node scripts/post-build.js",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "medusa start",
    "start:render": "medusa migrations run && medusa start",
    "start:server": "copy medusa-config-server.js medusa-config.js && medusa start",
    "start:worker": "copy medusa-config-worker.js medusa-config.js && medusa start",
    "dev": "medusa develop",
    "dev:server": "copy medusa-config-server.js medusa-config.js && medusa develop",
    "dev:worker": "copy medusa-config-worker.js medusa-config.js && medusa develop",
    "migrate": "medusa migrations run",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@medusajs/admin-sdk": "2.10.2",
    "@medusajs/cache-redis": "^2.10.2",
    "@medusajs/cli": "2.10.2",
    "@medusajs/framework": "2.10.2",
    "@medusajs/medusa": "2.10.2",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@types/redis": "^4.0.10",
    "awilix": "^8.0.1",
    "ioredis": "^5.7.0",
    "pg": "^8.13.0",
    "redis": "^5.8.2"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.10.2",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  }
}
